{
  "name": "agent-desktop-web",
  "version": "0.1.0",
  "description": "React Redux SSR boilerplate for agent desktop web",
  "author": "Nanang Suryadi <nanang.jobs@gmail.com>",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf yarn.lock && yarn install",
    "clean:prod": "rm -rf node_modules && rm -rf yarn.lock && yarn install --prod",
    "lint": "node_modules/.bin/eslint_d -c .eslintrc â€” ext .js src",
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "stop": "pkill signal SIGNIT agentApp"
  },
  "repository": "https://gitlab.com/suryakencana007/react-razzle-init",
  "license": "WTFPL",
  "jest": {
    "moduleNameMapper": {
      "kao-config(.*)$": "<rootDir>/config/$1",
      "kao-util(.*)$": "<rootDir>/src/utils/$1",
      "kao-server(.*)$": "<rootDir>/src/server/$1",
      "kao-store(.*)$": "<rootDir>/src/store/$1",
      "kao-redux(.*)$": "<rootDir>/src/redux/$1",
      "kao-components(.*)$": "<rootDir>/src/client/components/$1",
      "kao-client(.*)$": "<rootDir>/src/client/$1",
      "kao-containers(.*)$": "<rootDir>/src/client/containers/$1",
      "kao-assets(.*)$": "<rootDir>/src/client/assets/$1",
      "kao-styles(.*)$": "<rootDir>/src/client/styles/$1"
    }
  },
  "dependencies": {
    "antd": "^3.2.1",
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "clean-css": "^4.1.9",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "js-cookie": "^2.2.0",
    "path-to-regexp": "^2.1.0",
    "pug": "^2.0.0-rc.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-styled-components": "^1.4.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint_d": "^5.2.2",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-pug-plugin": "^0.2.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "minimist": "^1.2.0",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "razzle": "^0.8.7",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.19.1"
  }
}
